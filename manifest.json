{

  "manifest_version": 2,
  "name": "savefrom-is",
  "version": "1.0.20",

  "description": "Addon Savefrom.is for YT Video",

  "icons": {
    "64": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["sample/main.js"]
    }
  ],

  "applications": {
    "gecko": {
      "id": "addon.firefox.savefrom.is"
    }
  }

}
